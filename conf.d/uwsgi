UWSGI_USER=www
UWSGI_GROUP=www
UWSGI_EMPEROR_GROUP=www
UWSGI_EMPEROR_PATH=/etc/uwsgi/vassals/*.ini

# Additional options you might want to pass to uWSGI
#
UWSGI_EXTRA_OPTIONS='--ini /etc/uwsgi/uwsgi.conf'
